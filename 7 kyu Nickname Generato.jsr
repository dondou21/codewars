function nicknameGenerator(name){
  let third= name.split('');
  const vowels = 'aeuio';
  
  if(name.length < 4)
    {
      return 'Error: Name too short';
    }else {
      if(vowels.includes(third[2]))
        {
          return name.slice(0,4);
        }else
       return name.slice(0,3);
    }
  

}

console.log(nicknameGenerator('gregory'));
